	Tune.Turbochargers	        = 1		                -- Number of turbochargers in the engine, set to 0 for no turbochargers
		Tune.T_Boost			= .73 * 14.503773773	-- Boost in bar (edit the first number)
		Tune.T_ExPressFactor	= 10.8					-- Smoothness of the boost increase of the turbo
		Tune.T_ExPressInf		= 2.7					-- How much the turbo is influenced by the exhaust pressure
		Tune.T_SpoolTimeOffset	= .01					-- Offset in rpm for when the turbo spools
		Tune.T_Efficiency		= .06					-- Efficiency of the turbocharger
		Tune.T_WGMaxPress		= 2.9					-- Maximum pressure allowed by the wastegate
		Tune.T_SpoolFactor		= 1.94
		Tune.T_MinBoost			= 0						-- Minimum boost from the turbocharger
		Tune.T_SpoolIncrease	= .7					-- How quickly the turbo spools
		Tune.T_SpoolDecrease	= .95					-- How quickly the turbo stops spooling

	-- Supercharger
	Tune.Superchargers	        = 0		             -- Number of superchargers in the engine, Set to 0 for no superchargers
		Tune.S_Boost			= .56 * 14.503773773 -- Boost in bar (edit the first number)
		Tune.S_HighPerf			= 1.23
		Tune.S_StartBoost		= 8
		Tune.S_Efficiency		= .08
		Tune.S_Sensitivity		= .08

	--Misc
	Tune.ThrotAccel		= .05		-- Throttle acceleration, applied per tick (recommended values between .05 to .1)
	Tune.ThrotDecel		= .2		-- Throttle deceleration, applied per tick (recommended values between .1 to .3)

	Tune.BrakeAccel		= .05		-- Brake acceleration, applied per tick
	Tune.BrakeDecel		= .2		-- Brake deceleration, applied per tick

	Tune.RevAccel		= 200		-- RPM acceleration when clutch is on (this should be higher than RevDecay and RevDecayRPM)
	Tune.RevDecay		= 40		-- RPM decay when clutch is on
	Tune.RevDecayRPM	= 80		-- RPM decay from engine speed
	Tune.RevBounce		= 200		-- RPM kickback from redline

	Tune.Flywheel		= 500		-- Flywheel weight (higher = faster response, lower = more stable RPM)
	Tune.FlywheelEnergy	= 1000		-- How much torque per 1000 RPM the flywheel can produce when the clutch is dumped.

	Tune.Roughness		= 5		    -- Engine roughness (higher = more powerband jitter) (F2, V2, Split1, I2, F4, H4, V4, U4, I4, W6, I6, F8, V8, I8, F12, V12, U12, I12, I14, H16, V16, U16, W16, W18, H24, W24, all rotary engines and all wankel engines are smooth and I1, I3, V3, I5, V5, VR5, F6, V6, VR6, I7, W8, I9, WR10, WR12, V14, V20, W30 all X engines and all Radial Engines are not smooth)

	Tune.RevShake       = true      -- How much the chassis shakes when revving in neutral
	Tune.RevShakeForce  = 1         -- How intense the shaking is

	Tune.InclineComp	= 1		    -- Torque compensation multiplier for inclines (applies gradient from 0-90 degrees)

	Tune.IdleThrottle	= 2		    -- Percent throttle with no input. (in percent)

	Tune.Creep          = true      -- Creeping forwards at low speeds
	Tune.CreepStrength  = 200       -- Making this too low will result in no movement
	Tune.CreepSpeed     = 4         -- In km/h
