local PTW_Decimal_Place = 3  -- Rounds up to the desired decimal place

-- DO NOT TOUCH THE SCRIPT
local car = script.parent.parent.parent.Car.Value
local player=game.Players.LocalPlayer
local mouse = player:GetMouse()
local _Tune = require(car.Settings)
local MaxHP = 0
local MaxTQ = 0

while wait() do
	if math.floor(script.parent.parent.parent.Values.Horsepower.Value) > MaxHP then
		MaxHP = math.floor(script.parent.parent.parent.Values.Horsepower.Value)
		local PTWvalue = MaxHP/_Tune.Weight*10^PTW_Decimal_Place
		local Multiply = math.floor(PTWvalue)
		local PTW = Multiply/10^PTW_Decimal_Place

		script.Parent.PTW.Text = "PTW: "..PTW
	end
	script.Parent.HP.Text ="HP: ".. math.floor(script.parent.parent.parent.Values.Horsepower.Value)
	script.Parent.MAXHP.Text = "MaxHP: " .. MaxHP
	if math.floor(script.parent.parent.parent.Values.Torque.Value) > MaxTQ then
		MaxTQ = math.floor(script.parent.parent.parent.Values.Torque.Value * 1.3558179483)
		script.Parent.MAXTQ.Text = "MaxTQ: " .. MaxTQ
	end
	script.Parent.TQ.Text ="TQ: ".. math.floor(script.parent.parent.parent.Values.Torque.Value * 1.3558179483)
end]]></ProtectedString>
