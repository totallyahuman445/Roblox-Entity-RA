	FPV_ROTATION_X_MAX = math.pi / 7,

	WIND_SHAKE_FREQUENCY_MAX = .6, -- Speed of camera shake
	WIND_SHAKE_FREQUENCY_GROWTH = .00004, -- How speed of camera shake increases with speed
	WIND_SHAKE_AMPLITUDE_MAX = .15, -- Camera Shake Max
	WIND_SHAKE_AMPLITUDE_GROWTH = .00006, -- How much camera shake increases with speed

