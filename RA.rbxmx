		Tune.E_Redline				= 20000			        -- Electric max RPM
		Tune.E_DropSlope			= .114			        -- Electric torque decay slope
		Tune.E_TorqueDropRPM		= 5000			        -- When the torque starts decaying
		Tune.E_TorqueDropCurve		= 6.55
		Tune.E_TorqueDropSpread		= .767

