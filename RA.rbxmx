-- Settings
local Settings = {
	DEFAULT_VIEW_MODE = 0, --0: TPV, 1: FPV

	TOP_SPEED_THRESHOLD = 300, 

	TPV_ORIGIN_OFFSET_X = 0,
	TPV_ORIGIN_OFFSET_Y = 3,
	TPV_ORIGIN_OFFSET_Z = 0,

	TPV_OFFSET_Y = 3.5,

	TPV_OFFSET_MIN = 22, --studs magnitude
	TPV_OFFSET_MAX = 22,
	TPV_OFFSET_GROWTH = 0, --studs per unit velocity

