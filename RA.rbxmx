	Tune.BrakeForce		      = 2000											  -- Total brake force in kgf
	Tune.BrakeBias		      = .8												  -- Brake bias towards the front, percentage (1 = Front, 0 = Rear, .5 = 50/50)
	Tune.PBrakeForce	      = 3000											  -- Handbrake force in kgf
