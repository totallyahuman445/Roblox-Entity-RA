		Tune.Redline				= 7800			     -- Engine's max RPM
		Tune.IdleRPM				= 800			     -- Engine's idle RPM
		Tune.EqPoint				= 5252			     -- Standardized, do not touch
		Tune.StartRPM				= Tune.IdleRPM	     -- The RPM the engine starts at
		Tune.StartRPMAccel			= 30			     -- RPM increase to get to starting RPM per tick

		Tune.ShiftRPM				= 7200   	  	     -- RPM that the transmission shifts at
		Tune.PeakRPM				= Tune.ShiftRPM	     -- *PeakRPM is no longer functional in engine calculations.

		Tune.ValveSprings			= 9.5			     -- Higher values yield less valve float
		Tune.ValveFloatSharpness	= 3.7

		Tune.VVLSharpness			= .98			     -- Variable Valve Lift Sharpness
		Tune.VVLTime				= 5.4			     -- Rpm that the VVL activates at
		Tune.VVLValue				= 0			         -- Multiplier for VVL, set to 0 to deactivate (Recommended values between 0-.3)

		Tune.HighPerf				= -.8			     -- Higher values yield more high RPM performance
		Tune.LowPerf				= .5			     -- Higher values yield more low RPM performance
		Tune.LowPerfSpread			= .6			     -- The spread of the low RPM performance

		Tune.ExhaustPressure		= 1.1			     -- Engine exhaust pressure (for turbochargers)

	-- Electric Engine
	Tune.Electric		            = false
