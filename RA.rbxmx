		local alp = Settings.TPV_VERTICLE_FREQ * math.sqrt(1 - Settings.TPV_VERTICLE_DAMP * Settings.TPV_VERTICLE_DAMP)
		local exp = math.exp(-dt * omz)
		local cos = math.cos(dt * alp) 
		local sin = math.sin(dt * alp)
