	MANUAL_ROTATION_MAX = 4 * math.pi / 3,
	MANUAL_ROTATION_SPEED = .05,

	TPV_TILT_MAX = math.pi / 15,
	TPV_TILT_SPEED = .2,
	TPV_TILT_RETURN_SPEED = 1,

