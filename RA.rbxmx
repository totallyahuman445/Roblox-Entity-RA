local scale 			   = _Tune.Redline * .125
local redline              = _Tune.Redline  						 --Where you want the redline to be	
local rpmspan              = _Tune.Redline / scale				     --How many numbers you want the tachometer to display
local autoscaling          = false 									 --Estimates top speed for the speedometer, set to false for maxSpeed tuning

--Colour
local ATBGC               = Color3.new(1, .666667, 0) 		  --A/T Background Colour, Default: Color3.new(1,170/255,0)
