/_/ |_/_/  |_| GitHub:   https://github.com/totallyahuman445/Roblox-Entity-RA
			   THE List: https://docs.google.com/document/d/1WvOWw7ST4ZWpsCfXM3abJTTBkCA40NsayIEIdCRJOYM/edit?usp=sharing
			   Discord:  https://discord.gg/bS2A448Vrk
			   Log:		 https://docs.google.com/document/d/1SLW5NOAysg2rE6C52WEPnmOas_MNbeVNEv0xv2f__ps/edit?usp=sharing
			   Credits:  https://docs.google.com/document/d/1-mIgEhmugHfVUDz4vzZMS7XYMoEdLKedy0MedRqvbyA/edit?usp=sharing
]]
local hidemouse = true
local display   = true

local Plr = game.Players.LocalPlayer
local car = script.Parent.Parent.Car.Value
local cam = workspace.CurrentCamera
local RS = game:GetService("RunService")
local UserInputService = game:GetService"UserInputService"
local Plr = game.Players.LocalPlayer
local RunService = game:GetService"RunService"
local Mouse = Plr:GetMouse()
local Values = script.Parent.Parent.Values

Values.SteerT.Changed:Connect(function(Value)
	if Values.MouseSteerOn.Value and display then
		local Variable = workspace.CurrentCamera.ViewportSize.X / 3
		script.Parent.SteeringInput.Size = UDim2.new(0,Variable*Value,0,-10)
		script.Parent.SteeringInput.BackgroundColor3 = Color3.fromRGB(255*math.abs(Value),255*(1-math.abs(Value)),0)
	end
end)

car.DriveSeat.ChildRemoved:connect(function(child)
	if child.Name=="SeatWeld" then
		RS:UnbindFromRenderStep("MCam")
		cam.CameraType = Enum.CameraType.Custom
	end
end)

script.Parent.Parent.Values.MouseSteerOn.Changed:connect(function(property)
	UserInputService.MouseIconEnabled = false
	if script.Parent.Parent.Values.MouseSteerOn.Value then
		RS:BindToRenderStep("MCam",Enum.RenderPriority.Camera.Value,function()	
			cam.CameraType = Enum.CameraType.Scriptable
			local cc = car.DriveSeat.Position+Vector3.new(0,5+(2),0)-(car.DriveSeat.CFrame.lookVector*17)
			cam.CoordinateFrame = CFrame.new(cc,car.DriveSeat.Position)
		end)
	else
		UserInputService.MouseIconEnabled = true
		RS:UnbindFromRenderStep("MCam")
		cam.CameraType = Enum.CameraType.Custom
	end
end)]]></ProtectedString>
