local numbercolour        = Color3.new(1, 1, 1) --Number colour for tachometer and speedometer, Default: Color3.new(1, 1, 1)
local boostnumbercolour   = Color3.new(1, 1, 1) --Default: Color3.new(1, 1, 1)
local redlinenumbercolour = Color3.new(1, 1, 1) --Default: Color3.new(1, 0, 0)
local gearnumbercolour    = Color3.new(1, 1, 1) --Default: Color3.new(1, 1, 1)

local UNITS	= {
	--Click on speed to change units
	--First unit is default
	{
		units			= "km/h"			, -- Unit name
		scaling			= (11/12) * 1.09728	, -- 1 stud : 11 inches | ft/s to KP/H
		maxSpeed		= 370				, -- How much you want your speedometer to display
		spInc			= 40				, -- Increment between labelled notches
	},
	{
