							<ProtectedString name="Source"><![CDATA[-- Edit values in "Edit" module placed in "Ignition" localscript child. 

-- Don't change anything below this unless you know what you're doing

local module = require(script.Edit)
local car = script.Parent.Parent.Car.Value
local _Tune = require(car.Settings)
local RPM = script.Parent.Parent.Values.RPM
local isOn = script.Parent.Parent.IsOn
local mobile = false

local disengaged = true

if _Tune.AutoStart then
	disengaged = false
end

local Event = car:WaitForChild("IgnitionHandler")

local StarterSounds = {}
local randomizer = 1

local IGNITION_KEYBOARD = module.Keys.Keyboard
local IGNITION_CONTROLLER = module.Keys.Controller

local UserInputService = game:GetService("UserInputService")
local gamepadPresent = UserInputService.GamepadEnabled

function createSounds()
	table.clear(StarterSounds)
	for i, v in next, module.StarterSounds do
		local newsound = Instance.new("Sound")
		newsound.Name = v.name
		newsound.SoundId = "rbxassetid://"..v.id
		if v.name == "Starter" then
			newsound.Looped = true
		else
			newsound.Looped = false
		end
		newsound.Playing = false
		newsound.Volume = v.volume
		newsound.RollOffMinDistance = v.rollOffMin
		newsound.RollOffMaxDistance = v.rollOffMax
		newsound.Parent = car.Body[v.parent]
		table.insert(StarterSounds, i, newsound)
	end
end
function stopSounds()
	for i, v in next, StarterSounds do if v.Name == "Grind" then else v:Stop() end end
end
function removeSounds()
	for i, v in next, StarterSounds do v:Destroy() end
end

Event:FireServer("create", car, module)
car.DriveSeat.ChildRemoved:Connect(function(child)
	if child.Name == "SeatWeld" and child:IsA("Weld") then
		Event:FireServer("remove")
	end
end)

script.Parent.Parent.Starting.Changed:Connect(function(val)
	if val then
		disengaged = false
	end
end)

RPM.Changed:Connect(function(val)
	if val <= 400 and not isOn.Value and not script.Parent.Parent.Starting.Value and not disengaged then
		disengaged = true
		Event:FireServer("disengage")
	end
	if val <= 0 then
		disengaged = false
		stopSounds()
	end
end)

isOn.Changed:Connect(function(val)
	if isOn.Value then
		disengaged = false
		Event:FireServer("stop")
	end
end)

local function onInput(input:InputObject, gameProcessedEvent:boolean)
	if gameProcessedEvent then return end
	if not (input.KeyCode == IGNITION_KEYBOARD or input.KeyCode == IGNITION_CONTROLLER) then return end
	if script.Parent.Parent.IsOn.Value then
		if input.UserInputState == Enum.UserInputState.End then return end
		script.Parent.Parent.IsOn.Value = false
	else
		script.Parent.Parent.Starting.Value = input.UserInputState == Enum.UserInputState.Begin
		if (input.UserInputState == Enum.UserInputState.Begin) then
			if RPM.Value < _Tune.StartRPM then
				Event:FireServer("starter") 
			end
			if module.StarterGrinding then
				if RPM.Value >= 100 then
					randomizer = math.random(1, 2)
					if randomizer == 1 then
						Event:FireServer("grind")
					end
				end
			end
		else
			Event:FireServer("stop")
			stopSounds()
		end
	end
end

script.Parent.Parent.Mobile_Support.Startup.MouseButton1Down:Connect(function(mobilestart)
	mobile = true
	if not script.Parent.Parent.IsOn.Value then
		if RPM.Value < _Tune.StartRPM then
			Event:FireServer("starter") 
			script.Parent.Parent.Starting.Value = true
		end
		if module.StarterGrinding then
			if RPM.Value >= 100 then
				randomizer = math.random(1, 2)
				if randomizer == 1 then
					Event:FireServer("grind")
				end
			end
		end
	else
		script.Parent.Parent.IsOn.Value = false
	end
end)

script.Parent.Parent.Mobile_Support.Startup.MouseButton1Down:Connect(function(mobileend)
	mobile = true
	script.Parent.Parent.Starting.Value = false
end)

local function update()
	gamepadPresent = UserInputService.GamepadEnabled
	script.Parent.Text = "Hold <i><stroke color='#000000' thickness='2'><font weight='900'>"..(gamepadPresent and IGNITION_CONTROLLER.Name or IGNITION_KEYBOARD.Name).."</font></stroke></i> to turn on."
	if not mobile then
		script.Parent.Visible = not script.Parent.Parent.IsOn.Value
	else
		script.Parent.Visible = false
	end
end

UserInputService.InputBegan:Connect(onInput)
UserInputService.InputChanged:Connect(onInput)
UserInputService.InputEnded:Connect(onInput)

UserInputService.GamepadConnected:Connect(update)
UserInputService.GamepadDisconnected:Connect(update)
script.Parent.Parent.IsOn.Changed:Connect(update)
update()
]]></ProtectedString>
